# API Configuration for Mindful Steps

# Server Configuration
PORT=3001

# Vultr Object Storage Configuration
# Get these from your Vultr Object Storage dashboard
VULTR_ACCESS_KEY=your_vultr_access_key_here
VULTR_SECRET_KEY=your_vultr_secret_key_here
VULTR_BUCKET=mindful-steps
VULTR_REGION=ewr
VULTR_ENDPOINT=https://ewr.vultrobjects.com

# JWT Configuration (optional for production)
JWT_SECRET=your_jwt_secret_key_here

# Development settings
NODE_ENV=development